{
    "name": "singlestore/singlestore-laravel",
    "description": "A SingleStore database driver for Laravel.",
    "type": "library",
    "license": "Apache License 2.0",
    "autoload": {
        "psr-4": {
            "SingleStore\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SingleStore\\Laravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aarondfrancis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/container": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/events": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^8.5.23|^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
