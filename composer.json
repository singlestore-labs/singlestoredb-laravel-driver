{
    "name": "singlestoredb/singlestoredb-laravel",
    "description": "A SingleStoreDB database driver for Laravel.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "SingleStore\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SingleStore\\Laravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aarondfrancis@gmail.com"
        },
        {
            "name": "Carl Sverre",
            "email": "carl@singlestore.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/container": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.5.23|^9.5|^10.5|^11.5.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SingleStore\\Laravel\\SingleStoreProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
